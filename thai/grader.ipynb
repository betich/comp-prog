   "source": [
    "a = [int(e) for e in input().split()]\n",
    "k = int(input())\n",
    "\n",
    "b = []\n",
    "\n",
    "for e in a:\n",
    "  if len(b) > 0 and b[-1][0] == e:\n",
    "    b[-1].append(e)\n",
    "  else:\n",
    "    b.append([e])\n",
    "\n",
    "print(b)\n",
    "\n",
    "count = 0\n",
    "for e in b:\n",
    "  if not(len(e) >= k):\n",
    "    count += sum(e)\n",
    "\n",
    "print(count)"
   ]
